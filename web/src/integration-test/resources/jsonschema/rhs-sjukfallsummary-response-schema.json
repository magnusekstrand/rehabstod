{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Schema for sjukfall-summary as resturned from e.g /sjukfall-summary",
  "type": "object",
  "properties": {
    "total": {
      "type": "integer"
    },
    "men": {
      "type": "number"
    },
    "women": {
      "type": "number"
    },
    "groups": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "grupp": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string"
              },
              "kapitelList": {
                "type": "array",
                "items": {},
                "additionalItems": false
              },
              "name": {
                "type": "string"
              }
            },
            "additionalProperties": false
          },
          "count": {
            "type": "integer"
          }
        },
        "additionalProperties": false
      },
      "additionalItems": false
    }
  },
  "additionalProperties": false,
  "required": [
    "total",
    "men",
    "women",
    "groups"
  ]
}