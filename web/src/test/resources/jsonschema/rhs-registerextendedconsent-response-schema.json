{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Schema for RegisterExtendedConsent as resturned from e.g /consent",

    "properties": {
      "responseCode": {
        "type": "string",
        "enum": ["OK", "ERROR"]
      },
      "responseMessage": {
        "type": ["string", "null"]
      },
      "registeredBy": {
        "type": "string"
      }
    },
    "additionalProperties": false,
    "required": [
      "responseCode",
      "registeredBy"
    ],
    "type" : "object"
}